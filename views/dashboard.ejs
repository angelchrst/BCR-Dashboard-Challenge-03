    <h1><%= title_nav %></h1>
</div>
</div>
</div>
</header>


<div class="content">
    <div class="breadcrumb">
        <h1><%= title %></h1>
        <img src="img/arrow-right.svg" alt="arrow-right">
        <p><%= title %></p>
    </div>
    <h1 class="title-h1"><%= title %></h1>
    <div class="list-order">
        <img src="img/line-blue.svg">
        <h1>List Order</h1>
    </div>

    <!-- order table -->
    <div class="list-table">
        <table class="table table-borderless">
            <thead>
                <td style="text-align:center">No</td>
                <td>
                    User Email<img class="sort" src="img/sort.svg" alt="sort">
                </td>
                <td>Car<img class="sort" src="img/sort.svg" alt="sort"></td>
                <td>Start Rent<img class="sort" src="img/sort.svg" alt="sort"></td>
                <td>Finish Rent<img class="sort" src="img/sort.svg" alt="sort"></td>
                <td>Price<img class="sort" src="img/sort.svg" alt="sort"></td>
                <td>Status<img class="sort" src="img/sort.svg" alt="sort"></td>
            </thead>
            <tbody>
                <td style="text-align:center">
                    <% for(let i = 1; i <= 10; i++) { %>
                        <p><%= i %></p>
                    <% } %>
                </td>
                <td>
                    <% for(let i = 1; i <= 10; i++) { %>
                        <p>User Email</p>
                    <% } %>
                </td>
                <td>
                    <% for(let i = 1; i <= 10; i++) { %>
                        <p>Car</p>
                    <% } %>
                </td>
                <td>
                    <% for(let i = 1; i <= 10; i++) { %>
                        <p>Start Rent</p>
                    <% } %>
                </td>
                <td>
                    <% for(let i = 1; i <= 10; i++) { %>
                        <p>Finish Rent</p>
                    <% } %>
                </td>
                <td>
                    <% for(let i = 1; i <= 10; i++) { %>
                        <p>Price</p>
                    <% } %>
                </td>
                <td>
                    <% for(let i = 1; i <= 10; i++) { %>
                        <p>Status</p>
                    <% } %>
                </td>
            </tbody>
        </table>
    </div>

    <!-- table navigation -->
    <div class="table-nav">
        <div class="limit-nav">
            <p>Limit</p>
            <img src="img/form-drop.svg" alt="form-drop">
        </div>
        <div class="jump-nav">
            <p>Jump to page</p>
            <img src="img/form-drop-2.svg" alt="form-drop-2">
        </div>
        <div class="go-nav">
            <h1>Go</h1>
        </div>
        <div class="page-number">
            <img src="img/page-number.svg" alt="page-number">
        </div>
    </div>

    <div class="list-car">
