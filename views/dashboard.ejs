<%- include('partials/header') %>
<%- include('partials/navbar') %>
<!-- lanjutan navbar => buat tampilan active link di navbar side belum berhasil, jadi buat manual -->
<div class="frame-menu">
    <div class="dash-img" id="active">
        <a href="/dashboard">
            <img src="/img/dashboard.svg" alt="dashboard">
        </a>
        <a href="/dashboard">Dashboard</a>
    </div>
    <div class="dash-img">
        <a href="/cars">
            <img src="/img/cars.svg" alt="cars">
        </a>
        <a href="/cars">Cars</a>
    </div>
</div>
</div>
<div class="nav-white">
